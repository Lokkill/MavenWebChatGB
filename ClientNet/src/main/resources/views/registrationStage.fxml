<AnchorPane prefHeight="234.0" prefWidth="412.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.sample.controllers.RegistrationStage">
   <children>
      <Label layoutX="14.0" layoutY="14.0" text="Никнейм: ">
