            <Font size="20.0" />
         </font>
      </TextField>
      <TextField fx:id="txt_login" layoutX="108.0" layoutY="56.0">
         <font>
            <Font size="20.0" />
         </font>
      </TextField>
      <Button fx:id="btn_Ok" layoutX="80.0" layoutY="178.0" mnemonicParsing="false" onAction="#onOk" prefHeight="44.0" prefWidth="98.0" text="Ок">
